{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "defaultProject": "wavdio-angular",
  "newProjectRoot": "projects",
  "projects": {
    "wavdio-angular": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "de": {
              "localize": [
                "de"
              ]
            },
            "en": {
              "localize": [
                "en"
              ]
            },
            "es": {
              "localize": [
                "es"
              ]
            },
            "fr": {
              "localize": [
                "fr"
              ]
            },
            "production": {
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                }
              ],
              "buildOptimizer": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "outputPath": "dist",
              "sourceMap": false,
              "vendorChunk": false
            }
          },
          "options": {
            "aot": true,
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.webmanifest"
            ],
            "budgets": [
              {
                "maximumWarning": "6kb",
                "type": "anyComponentStyle"
              }
            ],
            "index": "src/index.html",
            "localize": true,
            "main": "src/main.ts",
            "ngswConfigPath": "ngsw-config.json",
            "outputPath": "dist/",
            "polyfills": "src/polyfills.ts",
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "node_modules/marked/lib/marked.js"
            ],
            "serviceWorker": true,
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/slick-carousel/slick/slick.css",
              "node_modules/slick-carousel/slick/slick-theme.css",
              "src/styles.css",
              "src/app-theme.scss"
            ],
            "tsConfig": "src/tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wavdio-angular:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": [
              "**/node_modules/**"
            ],
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "de": {
              "browserTarget": "wavdio-angular:build:de"
            },
            "en": {
              "browserTarget": "wavdio-angular:build:en"
            },
            "es": {
              "browserTarget": "wavdio-angular:build:es"
            },
            "fr": {
              "browserTarget": "wavdio-angular:build:fr"
            }
          },
          "options": {
            "browserTarget": "wavdio-angular:build",
            "proxyConfig": "proxy.conf.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.webmanifest"
            ],
            "karmaConfig": "src/karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/marked/lib/marked.js",
              "node_modules/slick-carousel/slick/slick.min.js"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/slick-carousel/slick/slick-theme.css",
              "node_modules/slick-carousel/slick/slick.css",
              "src/app-theme.scss",
              "src/styles.css"
            ],
            "tsConfig": "src/tsconfig.spec.json"
          }
        },
        "xliffmerge": {
          "builder": "@ngx-i18nsupport/tooling:xliffmerge",
          "options": {
            "xliffmergeOptions": {
              "defaultLanguage": "en",
              "genDir": "src/i18n",
              "i18nFormat": "xlf",
              "languages": [
                "de",
                "en",
                "es",
                "fr"
              ],
              "srcDir": "src/i18n"
            }
          }
        }
      },
      "i18n": {
        "locales": {
          "de": "src/i18n/messages.de.xlf",
          "en": "src/i18n/messages.en.xlf",
          "es": "src/i18n/messages.es.xlf",
          "fr": "src/i18n/messages.fr.xlf"
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {},
      "sourceRoot": "src"
    },
    "wavdio-angular-e2e": {
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "configurations": {
            "production": {
              "devServerTarget": "wavdio-angular:serve:production"
            }
          },
          "options": {
            "devServerTarget": "wavdio-angular:serve",
            "protractorConfig": "e2e/protractor.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "exclude": [
              "**/node_modules/**"
            ],
            "tsConfig": "e2e/tsconfig.e2e.json"
          }
        }
      },
      "prefix": "",
      "projectType": "application",
      "root": "e2e/"
    }
  },
  "version": 1
}
