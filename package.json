{
  "name": "wavdio-angular",
  "version": "2.1.0",
  "scripts": {
    "start": "node server.js",
    "start-de": "ng serve --configuration=de",
    "start-en": "ng serve --configuration=en",
    "start-es": "ng serve --configuration=es",
    "start-fr": "ng serve --configuration=fr",
    "build": "npm run build-de && npm run build-en",
    "build-de": "ng build --configuration=prod-de",
    "build-en": "ng build --configuration=prod-en",
    "build-es": "ng build --configuration=prod-es",
    "build-fr": "ng build --configuration=prod-fr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n wAVdio --i18n-format xlf --output-path i18n --i18n-locale en && ng run wAVdio:xliffmerge"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.9",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@ngx-i18nsupport/ngx-i18nsupport": "^1.1.6",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.9.1",
    "@types/jasminewd2": "^2.0.10",
    "@types/mongodb": "^3.6.20",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^8.10.66",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chai-shallow-deep-equal": "^1.4.6",
    "codelyzer": "^5.2.2",
    "jasmine-core": "^3.9.0",
    "jasmine-spec-reporter": "^4.2.1",
    "typescript": "~3.5.3"
  }
}
